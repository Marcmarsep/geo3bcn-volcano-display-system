html, body{
	width: 100%;
	height: 100%;
	margin: 0;
	padding: 0;
}

body{
	font-family: 'Oxygen', sans-serif;
	color: #424242;
}

select{
	margin-bottom: 15px;
}
label{
	font-size: 1.1em;
    color: #154415;
}

/*GENERAL*/
	.text-grey:hover{
		background: rgba(183,130,0,0.6);
	}
	.text-grey{
		color:#585757;
	}
	.text-grey2{
		color:#585757;
		background: #d4be34;
	}

	.text-white{
		color: #FFF;
	}

	.text-green-dark{
		color: #0d430e;
	}

	.text-green-light{
		color: #036e04;
	}

	.bg-white{
		background-color: #FFF;
	}

	.btn-action{
