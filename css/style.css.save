	}

	.footer-cont .cont-logos-footer{
		padding-right: 10px;
	}

	.footer-cont .cont-logos-footer .powered-by{
		font-size: .9em;
		margin-top: 5px;
	}

	.footer-cont .cont-logos-footer .logo-anchor{
		display: inline-block;
		max-width: 120px;
		margin: 0 10px 0 10px;
	}

